  "license": "MIT",
  "bin": {
    "debugtopus": "./dist/index.js"
  },
  "dependencies": {
    "@playwright/test": "^1.34.3",
    "axios": "^1.4.0",
    "commander": "^10.0.1",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  }
}
