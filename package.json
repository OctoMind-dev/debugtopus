{
  "name": "@octomind/debugtopus",
  "version": "3.7.0",
  "description": "Thin wrapper around playwright and the octomind api to run your automagically-maintained tests locally",
  "main": "./dist/index.js",
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "commonjs",
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "biome check",
    "build": "tsc --project tsconfig.build.json",
    "start": "npx tsx src/index.ts",
    "test": "jest --ci && pnpm test:playwright && pnpm test:cli",
    "test:playwright": "npx playwright test",
    "test:cli": "pnpm build && pnpm link . && debugtopus -h",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OctoMind-dev/debugtopus.git"
  },
  "author": "OctoMind-dev",
  "license": "MIT",
  "bin": {
    "debugtopus": "./dist/index.js"
  },
  "dependencies": {
    "@octomind/debugtopus": "link:",
    "@playwright/test": "1.55.0",
    "axios": "1.11.0",
    "commander": "14.0.0",
    "otplib": "12.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@tsconfig/node-lts": "22.0.2",
    "@types/jest": "29.5.14",
    "@types/node": "24.3.0",
    "@types/tmp": "0.2.6",
    "jest": "30.0.5",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  }
}
